#pierwszy plik
print("CDV")
print(2)

#potegowanie
potega = 2 ** 10
print(potega)

tekst = "CDV"
print(tekst * 2)

#pobieranie danych z klawiatury
imie = input()
print("Twoje imiÄ™: " + imie)

nazwisko = 'Nowak'
print('Twoje dane: ' + imie + ' ' + nazwisko)

dlugosc = len(nazwisko)
print(dlugosc)
print(dlugosc)
print(type(dlugosc))
dlugosc = str(dlugosc)
print('dlugosc nazwiska: ' + dlugosc)

nazwisko = 'kowalski'
pierwszyZnak = nazwisko[0]
print(pierwszyZnak)

ostatniZnak = nazwisko[len(nazwisko)-1]
print(ostatniZnak)

ostatniZnak = [-1]
print(ostatniZnak)

#konwersja
x = "5"
print(type(x))
x = int(x)
print(type(x))
x = float(x)
print(type(x))
print(x)

y = 5
print(type(y))
y = y / 2
print(type(y))

nazwisko = 'kowalski'
print(nazwisko[0])
print(nazwisko[0:3])
print(nazwisko[-2})
print(nazwisko[-2:])